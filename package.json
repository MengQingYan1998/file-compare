{
  "name": "file-compare",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "MengQingYan",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:electron": "set NODE_ENV=development && electron .",
    "build:electron": "npm run build && electron-builder",
    "start": "electron ."
  },
  "dependencies": {
    "decimal.js": "^10.6.0",
    "element-plus": "^2.11.8",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "electron": "29.4.6",
    "electron-builder": "^26.0.12",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-electron": "^0.29.0",
    "vue-tsc": "^3.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "build": {
    "appId": "com.vite-electron",
    "productName": "文件对比",
    "copyright": "Copyright © 2025 vite",
    "nsis": {
      "oneClick": false,  
      "allowToChangeInstallationDirectory": true 
      
